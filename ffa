setclipboard("https://discord.gg/ZbKkP2X7ps")





if not game:IsLoaded() then game.Loaded:Wait() end

local lp = game:GetService("Players").LocalPlayer
local pGui = lp:WaitForChild("PlayerGui")
local HttpService = game:GetService("HttpService")
local TeleportService = game:GetService("TeleportService")


local JEW_LIMIT = 200 
local MIN_NIGGERS = 7
local PLACE_ID = game.PlaceId

local fullServerBlacklist = {}
fullServerBlacklist[game.JobId] = true 
local function Hop(reason)
    print("!! TRIGGERED !! Reason: " .. reason)
    
    local function fetchAndTeleport()
        local url = 'https://games.roblox.com/v1/games/' .. PLACE_ID .. '/servers/Public?sortOrder=Desc&limit=100'
        local success, result = pcall(function()
            return HttpService:JSONDecode(game:HttpGet(url))
        end)

        if success and result and result.data then
            for _, s in pairs(result.data) do
                if not fullServerBlacklist[s.id] and tonumber(s.playing) < tonumber(s.maxPlayers) then
                    print("Attempting join: " .. s.id)
                    local tpSuccess, tpError = pcall(function()
                        TeleportService:TeleportToPlaceInstance(PLACE_ID, s.id, lp)
                    end)
                    
                    if not tpSuccess then
                        fullServerBlacklist[s.id] = true
                        task.wait(1)
                    else
                        return true 
                    end
                end
            end
        end
        return false
    end

    while true do
        if fetchAndTeleport() then break end
        task.wait(3)
    end
end
local function forceClick(obj)
    if not obj then return end
    pcall(function()
        local signals = {"MouseButton1Click", "Activated"}
        for _, sig in pairs(signals) do
            local conns = getconnections(obj[sig])
            for _, conn in pairs(conns) do
                conn:Fire()
                task.wait(0.1) 
            end
        end
        obj:Activate()
    end)
end
  task.spawn(function()
    task.wait(15) 
    while true do
        task.wait(5) 
        local activePlayers = 0
        local jewFound = false
        for _, v in pairs(pGui:GetDescendants()) do
            if v:IsA("TextLabel") and v.Visible then
                
                
                local cleanText = v.Text:gsub("%D", "")
                local val = #cleanText > 0 and tonumber(cleanText)
                
                if val then
                    if val >= JEW_LIMIT then
                        if v.Parent.Name:lower():find("slot") or v.Parent.Name:lower():find("score") or v.Name:lower():find("points") then
                            jewFound = true
                        end
                    end
                end

                if v.Text:find("@") and v.Text ~= "" then
                    activePlayers = activePlayers + 1
                end
            end
        end
        
        print("Status: " .. activePlayers .. " players found. Jew: " .. tostring(jewFound))
        
        if jewFound then
            Hop("Score limit reached.")
            break
        elseif activePlayers > 0 and activePlayers < MIN_NIGGERS then
            Hop("Server too empty (" .. activePlayers .. " players).")
            break
        end
    end
end)
task.spawn(function()
    task.wait(5)
    for _, v in pairs(pGui:GetDescendants()) do
        if v:IsA("TextLabel") and v.Text:find("Free For All") then
            forceClick(v:FindFirstAncestorOfClass("GuiButton") or v)
            break
        end
    end
    task.wait(3)
    for _, v in pairs(pGui:GetDescendants()) do
        if v:IsA("TextLabel") and v.Text:find("Join this game!") then
            forceClick(v.Parent:FindFirstChildOfClass("GuiButton") or v:FindFirstAncestorOfClass("GuiButton"))
            break
        end
    end
end)


Notify("Script loaded, discord copied to clipboard") 


